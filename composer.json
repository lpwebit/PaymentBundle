{
  "name": "lpwebit/payment-bundle",
  "type": "symfony-bundle",
  "description": "A Symfony2 Bundle for making payment",
  "keywords": ["symfony2", "payment", "paypal"],
  "homepage": "https://github.com/lpwebit/PaymentBundle",
  "license": "MIT",
  "authors": [
    {
      "name": "Lorenzo Pezzali",
      "email": "lorenzo.pezzali@lpweb.it"
    },
    {
      "name": "PaymentBundle Contributors",
      "homepage": "https://github.com/lpwebit/PaymentBundle/graphs/contributors"
    }
  ],
  "require": {
    "php": ">=5.3.9",
    "paypal/rest-api-sdk-php": "*"
  },
  "require-dev": {
    "symfony/symfony": "2.8.*",
    "doctrine/orm": "^2.4.8",
    "doctrine/doctrine-bundle": "~1.4",
    "swiftmailer/swiftmailer": "~4.3|~5",
    "symfony/monolog-bundle": "^2.10",
    "symfony/security-bundle": "~2.3|~3.0",
    "sensio/framework-extra-bundle": "^3.0.2",
    "symfony/twig-bundle": "~2.3|~3.0",
    "symfony/console": "~2.3|~3.0",
    "symfony/phpunit-bridge": "~2.7|~3.0",
    "symfony/validator": "~2.3|~3.0",
    "symfony/yaml": "~2.3|~3.0",
    "sensio/generator-bundle": "~2.3|^3.0",
    "phpunit/phpunit": "4.8.*"
  },
  "autoload": {
    "psr-0": {
      "LpWeb\\PaymentBundle": ""
    }
  },
  "target-dir": "LpWeb/PaymentBundle"
}
